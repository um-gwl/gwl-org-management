{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/no-multi-comp": [
      0,
      {
        "ignoreStateless": true
      }
    ],
    "jsx-a11y/href-no-hash": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 1,
    "import/no-unresolved": 0,
    "import/extensions": 0
  }
}